  - path: ../../documentation/example/mikroe_e_paper_154_inch/image/create_example.png
    directory: image
  - path: ../../documentation/example/mikroe_e_paper_154_inch/image/result.png
    directory: image
ui_hints:
  highlight:
    - path: README.md
      focus: true